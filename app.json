{
  "expo": {
    "name": "water sensor app",
    "slug": "water-sensor-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoSplash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/LogoSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.watersensor.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoSplash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.watersensor.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./local/assets/notification_sound.wav",
            "./local/assets/notification_sound_other.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "051b409b-ee75-41db-9fef-078d6cdc3f37"
      }
    }
  }
}